		"sources": {
			"VenditeLib.sol": {
				"content": "// SPDX-License-Identifier: MIT\r\npragma solidity ^0.8.0;\r\n\r\nlibrary ValidatorLibrary {\r\n\r\n    struct ValidatorData {\r\n        address[] validators;\r\n        mapping(address => bool) isValidator;\r\n    }\r\n\r\n    function numberOfValidators(ValidatorData storage self) internal view returns (uint) {\r\n        return self.validators.length;\r\n    }\r\n\r\n    function isRegistered(ValidatorData storage self, address validator) internal view returns (bool) {\r\n        return self.isValidator[validator];\r\n    }\r\n\r\n    function isNewsConfirmed(\r\n        ValidatorData storage self,\r\n        address newsAddress,\r\n        uint minValidations,\r\n        mapping(address => bool) storage newsValidations\r\n    ) internal view returns (bool) {\r\n        uint validationCount = 0;\r\n        for (uint i = 0; i < self.validators.length; i++) {\r\n            if (newsValidations[self.validators[i]]) {\r\n                validationCount++;\r\n            }\r\n        }\r\n        return validationCount >= minValidations;\r\n    }\r\n}\r\n"
			}
		},
